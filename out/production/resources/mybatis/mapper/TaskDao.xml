<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zb.dao.TaskDao">

    <insert id="insertList">
        INSERT INTO `t_task` (`business_id`, `date_task`, `name_task`,`keywords`, `spec`, `require`, `time_doing`, `buy_num`, `price`)
        values
        <foreach collection="list" item="task" separator=",">
            (#{task.businessId}, #{task.dateTask}, #{task.nameTask}, #{task.keywords}, #{task.spec}, #{task.require}, #{task.timeDoing}, #{task.buyNum}, #{task.price})
        </foreach>
    </insert>

    <select id="queryPageableData" resultType="com.zb.dto.CommissionDto" parameterType="com.zb.dto.CommissionDto">
        select t.id,t.business_id as businessId, t.date_task as dateTask, t.name_task as nameTask, t.keywords, t.spec, t.require, t.time_doing as timeDoing, t.buy_num as buyNum, t.price,
        t.create_time as createTime, t.update_time as updateTime from t_task t
        <where>

        </where>

    </select>

</mapper>